class Solution {
    public int maximumSwap(int num) {
        String s=Integer.toString(num);
        int n=s.length();
        int tempNum=num;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                char ch[]=s.toCharArray();
                char temp=ch[i];
                ch[i]=ch[j];
                ch[j]=temp;
                tempNum=Math.max(tempNum,Integer.parseInt(new String(ch)));
            }
        }
        return tempNum;
    }
}
